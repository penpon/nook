{
	"name": "vite-react-typescript-starter",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,json}\"",
		"format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,json}\"",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"coverage": "vitest run --coverage",
		"audit": "npm audit fix",
		"check": "npm run format:check && npm run lint",
		"quality-check": "npm run format:check && npm run lint && npm run test:run && npm run coverage",
		"quality-fix": "npm run format && npm run lint:fix && npm run test:run && npm run coverage && npm run audit",
		"prepare": "husky"
	},
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.0",
		"@mui/icons-material": "^7.2.0",
		"@mui/material": "^7.2.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"axios": "^1.6.7",
		"date-fns": "^3.3.1",
		"lucide-react": "^0.344.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-markdown": "^9.0.1",
		"react-query": "^3.39.3",
		"recharts": "^3.6.0",
		"remark-gfm": "^4.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.9.1",
		"@tailwindcss/typography": "^0.5.10",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/react": "^18.3.5",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"@vitest/coverage-v8": "^4.0.16",
		"@vitest/ui": "^4.0.16",
		"autoprefixer": "^10.4.23",
		"eslint": "^9.9.1",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.11",
		"eslint-plugin-security": "^3.0.1",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"jsdom": "^26.1.0",
		"lint-staged": "^16.2.7",
		"postcss": "^8.4.35",
		"prettier": "^3.7.4",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.5.3",
		"typescript-eslint": "^8.3.0",
		"vite": "^7.2.7",
		"vite-plugin-pwa": "^1.0.1",
		"vitest": "^4.0.16"
	}
